let inicializaReddis = () : redis.RedisClient =>{
    const client = redis.createClient(
        objConfig.arrConfig_RedisServer.intPort,
        objConfig.arrConfig_RedisServer.strHost
      );
 
    client.on("error", function(error) {
      console.error(error);
    });

    return client;
}